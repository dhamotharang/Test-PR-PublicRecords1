export Function_CareOfName(string pStringIn) :=
    if(stringlib.stringfind(PStringIn,'C/O',1)!=0,
     if(stringlib.stringfind(pStringIn,'#',1)=0 AND stringlib.stringfind(pStringIn,', UNIT ',1)=0,	 
      if(stringlib.stringfind(PStringIn,',',7)!=0,
       trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',5)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',6)!=0,
       trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',4)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',5)!=0,
	   trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',3)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',4)!=0,
	   trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',2)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',3)!=0,
	   trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',1)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',2)!=0,
	   trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',1)-1],left,right),
	  '')))))),
	 if(stringlib.stringfind(pStringIn,'#',1)!=0 OR stringlib.stringfind(pStringIn,', UNIT ',1)!=0,
	  if(stringlib.stringfind(PStringIn,',',7)!=0,
       trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',4)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',6)!=0,
       trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',3)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',5)!=0,
	   trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',2)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',4)!=0,
	   trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',1)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',3)!=0,
	   trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',1)-1],left,right),
	  if(stringlib.stringfind(PStringIn,',',2)!=0,
	   trim(PStringIn[stringlib.stringfind(PStringIn,'C/O',1)+4..stringlib.stringfind(PStringIn,',',1)-1],left,right),
	  '')))))),'')),'');
	 
	 