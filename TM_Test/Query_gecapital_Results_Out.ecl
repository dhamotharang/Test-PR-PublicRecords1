// Join to BDID Table to get stats
gecapital_stats := join(Business_Risk.BDID_Table,
                     bdid_list,
				 left.bdid = right.bdid,
				 transform(layout_gecapital_combined, self.ACCOUNT := right.ACCOUNT, self := left),
				 hash);

brt := Business_Risk.BDID_Risk_Table;
			
layout_gecapital_combined AppendRiskData(layout_gecapital_combined l, brt r) := transform
self := r;
self := l;
end;

			
// Append scores
gecapital_score := join(gecapital_stats,
                        brt,
				    left.bdid = right.bdid,
				    AppendRiskData(left, right),
				    hash);
				
				
