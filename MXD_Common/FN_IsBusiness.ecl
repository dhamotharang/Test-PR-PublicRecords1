export FN_IsBusiness(UNICODE name) := FUNCTION

regexBusiness :=u'(ADECV|ASOCIACION|ASOCIAD|CULTURAL|IMPORT|EXPORT|BANCA|CREDIT|COMERCI|SERVICI|INDUST|SANEAMIEN|SDERL|EMPRESA|INDEPENDI|CALHIDRA|HIPOTEC|SECRETA'
+ u'|BANCO|FEDERAL|PUBLICO|ARRENDAD|INSTITUTO|CONSTRUC|EDIFICAC|AYUNTAMIENTO|INMOBILIARIA|MUNICIPIO|COLEGIO|DISTRITO|'
+ u'COMERCIA|SOCIEDAD|ESTADO|ESCUELA|GRUPO|SINDICATO|COMPAÃ‘IA|NACIONAL|CONSTRUCTORA|CORPORATIVO|CENTRODE|JURISDICCION|PRODUCT|'
+ u'BANK|SOCIAL|URBANIS|DEDELICIAS|FONDO|NACIONAL|LIBERTAD|CDP|CFE|AGUASCALIENTES|DECHIHUAHUA|DELNORTE|'
+ u'EJIDO|COMISION|HOSPITAL|UNIVERSIDAD|DELEGACION|COOPERATIVA|REGISTRY|CIVIL|TAMAULIPAS|MEXICO|FEDERACION|SACV|SRLDECV|SRLCV|FUNDACION|SADERL|BUSINESS|SPRL|RLDECV|SDERL|BANAMEX|CITIBANK|BANKOF)';

namestr :=TRIM(name,all);

ns1:= UnicodeLib.UnicodeFindReplace(namestr,u'.',u'');
ns2:= UnicodeLib.UnicodeFindReplace(ns1,u',',u'');
ns3:= UnicodeLib.UnicodeFindReplace(ns2,u'-',u'');
isBusiness :=REGEXFIND(regexbusiness,ns3,0) != u'';


return isBusiness ;

END;