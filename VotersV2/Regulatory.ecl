  export applyVoters(base) := functionmacro
    import suppress, votersv2;
    
    Base_File_Append_In := suppress.applyregulatory.getFile('file_voters_inj.txt', votersv2.regulatory.base_layout);

    UNSIGNED6 endMax := MAX(Base, vtid);

    recordof(base) reformat_append(Base_File_Append_In L, INTEGER c) := 
        transform
            self.rid := (unsigned)L.rid;
            self.did := (unsigned)l.did;
            self.did_score := (unsigned)l.did_score;
            self.vtid := endMax + c;
            self := L;
            self := [];
        end;

    Base_File_Append := project(Base_File_Append_In, reformat_append(left,COUNTER));															

    return base + Base_file_append;						
  
  endmacro;
