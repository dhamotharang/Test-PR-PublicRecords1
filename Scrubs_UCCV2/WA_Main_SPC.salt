FIELDTYPE:invalid_filing_jurisdiction:ENUM(WA|WA)
FIELDTYPE:invalid_orig_filing_number:CUSTOM(Scrubs_UCCV2.Functions.fn_non_empty_numeric>0)
FIELDTYPE:invalid_orig_filing_type:CUSTOM(Scrubs_UCCV2.Functions.fn_wa_orig_filing_type>0)
