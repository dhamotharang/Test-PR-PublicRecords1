FIELDTYPE:invalid_fname:ALLOW(AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz-.' )
FIELDTYPE:invalid_lname:ALLOW(1234AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz-.' )
FIELDTYPE:invalid_addr:ALLOW(0123456789AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz-.'#/ ):CUSTOM(Scrubs_Thrive.Functions.fn_invalid_addr >0)
FIELDTYPE:invalid_city:ALLOW(AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz-.' )
FIELDTYPE:invalid_state:CUSTOM(Scrubs.Functions.fn_Valid_StateAbbrev>0)
FIELDTYPE:invalid_zip5:CUSTOM(Scrubs.Functions.fn_valid_zip >0)
FIELDTYPE:invalid_zip4:CUSTOM(Scrubs.Functions.fn_numeric_optional >0)
FIELDTYPE:invalid_email:ALLOW(0123456789AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz-.@_,;'&)
FIELDTYPE:invalid_phone:CUSTOM(Scrubs.Functions.fn_verify_phone>0,TRUE)
FIELDTYPE:invalid_employer:ALLOW(0123456789AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz-_,;'&./+()#!:%& ):LENGTHS(1..)
FIELDTYPE:invalid_date:ALLOW(0123456789/: ):CUSTOM(Scrubs_Thrive.Functions.fn_invalid_date>0)
//--------------------------------------------------------------- 
//FIELDS TO SCRUB -- Commented out fields are not being scrubbed
//---------------------------------------------------------------
FIELD:orig_fname:TYPE(STRING):LIKE(invalid_fname):0,0
FIELD:orig_lname:TYPE(STRING):LIKE(invalid_lname):0,0
FIELD:orig_addr:TYPE(STRING):LIKE(invalid_addr):0,0
FIELD:orig_city:TYPE(STRING):LIKE(invalid_city):0,0
FIELD:orig_zip4:TYPE(STRING):LIKE(invalid_zip4):0,0
FIELD:orig_state:TYPE(STRING):LIKE(invalid_state):0,0
FIELD:orig_zip5:TYPE(STRING):LIKE(invalid_zip5):0,0
FIELD:email:TYPE(STRING):LIKE(invalid_email):0,0
FIELD:phone:TYPE(STRING):LIKE(invalid_phone):0,0
