FIELDTYPE:invalid_ok_wkp_id:CUSTOM(Scrubs_OneKey.Functions.fn_onekey_id>0)
FIELDTYPE:invalid_city_nm:CUSTOM(Scrubs_OneKey.Functions.fn_populated_strings>0)
