	//********************************************************************
		//STATUSDESC: Returns the desc to "CORP_STATUS_DESC" 
	//********************************************************************	
	EXPORT  StatusDesc(STRING code) := FUNCTION

		st  := corp2.t2u(code);
		RETURN case(st,
								'A' => 'ADMINISTRATIVELY DISSOLVED',
								'B' => 'ADMINISTRATIVELY REVOKED',
								'C' => 'CURRENT',
								'D' => 'DISSOLVED',														
