import tools;

export Files(

	 string		pversion = ''
	,boolean	pUseProd = false

) :=
module

	//////////////////////////////////////////////////////////////////
	// -- Input File Versions
	//////////////////////////////////////////////////////////////////
	tools.mac_FilesInput(Filenames(pversion,pUseProd).Input,	layouts.Input.Sprayed,	Input,	'Flat');
		
	//////////////////////////////////////////////////////////////////
	// -- Base File Versions
	//////////////////////////////////////////////////////////////////
	tools.mac_FilesBase	(Filenames(pversion,pUseProd).Base,	Layouts.Base,	Base);
	
	
end;