import versioncontrol;
export Files(string pversion = '') := module

   //////////////////////////////////////////////////////////////////
   // -- Input File Versions
   //////////////////////////////////////////////////////////////////
  
    versioncontrol.macInputFileVersions(Filenames(pversion).input, layouts.Input, Input,'CSV',,'\n','|',1,_Dataset().max_record_size);
   
   //////////////////////////////////////////////////////////////////
   // -- Base File Versions
   //////////////////////////////////////////////////////////////////
   versioncontrol.macBuildFileVersions(Filenames(pversion).Base, layouts.Base, Base);
	 
   //////////////////////////////////////////////////////////////////
   // -- KeyBuild File
   //////////////////////////////////////////////////////////////////
   export KeyBuild :=	dataset(_Dataset().thor_cluster_files + 'temp::' + _Dataset().name + '::' + pversion + '::allStates',Diversity_Certification.Layouts.keybuild,flat);
   
   //////////////////////////////////////////////////////////////////
   // -- KeyBuild SuperFile
   //////////////////////////////////////////////////////////////////
   export KeyBuildSF :=	dataset(_Dataset().thor_cluster_files + 'keybuild::' + _Dataset().name + '::qa::data',Diversity_Certification.Layouts.keybuild,flat);
	 
	 end;
	 
	 