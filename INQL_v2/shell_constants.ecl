export age1day(string logdate, unsigned3 historydate) := function
	today := risk_indicators.iid_constants.mygetdate(historydate);
	good_date := trim(logdate)<>'' and (unsigned)logdate <= (unsigned)today;
	days := ut.DaysApart( logdate, today );
	return good_date and days <= 1; // may need to change this to be 2 days to allow for build time if we ever want this field populated
end;

export age1week(string logdate, unsigned3 historydate) := function
	today := risk_indicators.iid_constants.mygetdate(historydate);
	good_date := trim(logdate)<>'' and (unsigned)logdate <= (unsigned)today;
	days := ut.DaysApart( logdate, today );
	return good_date and days <= 7;
end;

// ccyymmdd hhmmssms
// 20130925 20550100
export age_in_days_using_timestamp(integer day_threshold, string logdate, string historydateTimeStamp, unsigned3 historydate) := function
	today := risk_indicators.iid_constants.mygetdateTimeStamp(historydateTimeStamp, historydate);
	
	daynow := today[1..8];
	timenow := today[9..17];
	
	logday := logdate[1..8];
	logtime := logdate[9..17];
	
	populated := trim(logdate)<>'';
	previous := (unsigned)logday < (unsigned)daynow or  // prior to today 
	((unsigned)logday = (unsigned)daynow  and (unsigned)logtime <= (unsigned)timenow );  // same day but inquiry timestamp was prior to time right now

	good_date :=  populated and (previous or historydate=risk_indicators.iid_constants.default_history_date) ;  // don't need to check for 'previous' when query is running in realtime mode
	days := ut.DaysApart( logday, daynow ); // potential future enhancement: figure out this calculation using time as well to get a true 24 hour bucket cutoff in the 1 day threshold
	
	return good_date and days <= day_threshold; 
end;

export hist_is_ok(string logdate, string historydateTimeStamp, unsigned3 historydate, integer bsversion) := function
	today := risk_indicators.iid_constants.mygetdateTimeStamp(historydateTimeStamp, historydate);
	daynow := today[1..8];
	timenow := today[9..17];
	
	logday := logdate[1..8];
	logtime := logdate[9..17];
	
	populated := trim(logdate)<>'';
	previous := (unsigned)logday < (unsigned)daynow or  // prior to today 
	((unsigned)logday = (unsigned)daynow  and (unsigned)logtime <= (unsigned)timenow - 150000 );  // same day but inquiry timestamp was 15 minutes prior to time right now

	//  need to maintain both checks because old queries are not passing anything in for historydateTimeStamp
	histOK_41 := (unsigned)logdate[1..6]<historydate;
	histOK_50 := populated and (previous or historydate=risk_indicators.iid_constants.default_history_date) ;  // don't need to check for 'previous' when query is running in realtime mode
	
	histOK := if(bsversion>=50, histOK_50, histOK_41);
	
	return histOK;
end;


