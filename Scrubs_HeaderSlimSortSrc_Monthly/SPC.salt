FIELD:sec_range_cnt_with_this_nm_addr:TYPE(UNSIGNED2):0,0
FIELD:ssn_cnt_with_this_nm_addr:TYPE(UNSIGNED2):0,0
FIELD:dob_cnt_with_this_nm_addr:TYPE(UNSIGNED2):0,0
FIELD:mname_cnt_with_this_nm_addr:TYPE(UNSIGNED2):0,0
