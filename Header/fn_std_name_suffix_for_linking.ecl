export fn_std_name_suffix_for_linking(string s) :=
  MAP( (integer)s = 1 => 'I',
       (integer)s = 2 => 'JR',
       (integer)s = 3 => 'III',
       (integer)s = 4 => 'IV',
       (integer)s = 5 => 'V',
       (integer)s = 6 => 'VI',
	   (integer)s = 7 => 'VII',
	   (integer)s = 8 => 'VIII',
	   (integer)s = 9 => 'IX',
       s = 'IIII'  => 'IV',
	   s = 'II'    => 'JR',
	   s = 'THIRD' => 'III',
       s = 'SECOND'=> 'JR',
       s = '2ND'   => 'JR', 
       s = '3RD'   => 'III',
       s = '9TH'   => 'IX',
       s = '4TH'   => 'IV',
       s = '5TH'   => 'V',
       s = '6TH'   => 'VI',
       s = '7TH'   => 'VII',
       s = '8TH'   => 'VIII',
       s = '10TH'  => 'X',
	   s = '11TH'  => 'XI',
       s = '12TH'  => 'XII',
       s );