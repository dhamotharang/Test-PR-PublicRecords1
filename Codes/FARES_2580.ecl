	codes.Layout_Codes_V3 trans(codes.File_Codes_V3_in le) :=
		TRANSFORM
			translation := 
				MAP(le.field_name = 'FOUNDATION'	=>	FOUNDATION(le.field_name2, le.code),
				    le.field_name = 'LAND_USE'	=>	LAND_USE(le.field_name2, le.code),
                        le.field_name = 'HEATING' => HEATING(le.field_name2,le.code),
				    le.field_name = 'STYLE' => STYLE(le.field_name2,le.code),
				    le.field_name = 'ROOF_TYPE' => ROOF_TYPE(le.field_name2,le.code),
				    le.field_name = 'ROOF_COVER' => ROOF_COVER(le.field_name2,le.code),
				    le.field_name = 'MORTGAGE_DEED_TYPE' => MORTGAGE_DEED_TYPE(le.field_name2,le.code),
				    le.field_name = 'AIR_CONDITIONING' => AIR_CONDITIONING(le.field_name2, le.code),
				    le.field_name = 'FUEL' => FUEL(le.field_name2,le.code),
				    le.field_name = 'MORTAGE_TERM_CODE' => MORTGAGE_TERM_CODE(le.code),
				    '');
				  
			SELF.code := IF(stringlib.StringCleanSpaces(le.long_desc)=
                               stringlib.StringCleanSpaces(translation),SKIP,le.code);
			SELF := le;
		END;
	
	p := PROJECT(codes.File_Codes_V3_in(file_name='FARES_2580', 
