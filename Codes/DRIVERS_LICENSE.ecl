	FUNCTION
		
	codes.Layout_Codes_V3 trans(codes.File_Codes_V3_in le) :=
		TRANSFORM
			translation := 
				MAP(le.field_name = 'ATTENTION_FLAG'	=>	ATTENTION_FLAG(le.field_name2, le.code),
				    le.field_name = 'EYE_COLOR'	=>	EYE_COLOR(le.field_name2, le.code),
				    le.field_name = 'HAIR_COLOR'	=>	HAIR_COLOR(le.field_name2, le.code),
				    le.field_name = 'HISTORY'	=>	HISTORY(le.code),
				    le.field_name = 'ISSUANCE'	=>	ISSUANCE(le.field_name2, le.code),
				    le.field_name = 'LICENSE_TYPE'	=>	LICENSE_TYPE(le.field_name2, le.code),
				    le.field_name = 'LIC_ENDORSEMENT'	=>	LIC_ENDORSEMENT(le.field_name2, le.code),
				    le.field_name = 'RESTRICTIONS'	=>	RESTRICTIONS(le.field_name2, le.code),
				    le.field_name = 'RESTRICTIONS_DELIMITED'	=>	RESTRICTIONS_DELIMITED(le.field_name2, le.code),
                        '');
				  
			SELF.code := IF(stringlib.StringCleanSpaces(le.long_desc)=
                               stringlib.StringCleanSpaces(translation),SKIP,le.code);
			SELF := le;
		END;
	
	p := PROJECT(codes.File_Codes_V3_in(file_name='DRIVERS_LICENSE'),trans(LEFT));
	RETURN p;
		
