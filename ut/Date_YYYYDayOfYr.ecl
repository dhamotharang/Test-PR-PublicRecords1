////////////////////////////////////////////////////////////////////////////////////////////////////////////
//////////////// INSTRUCTIONS:
////////////////	First Input - YYYY
////////////////	Second Input - DDD
////////////////////////////////////////////////////////////////////////////////////////////////////////////

export string Date_YYYYDayOfYr(integer2 year,integer2 day) := 
			(string)intformat(
				(year * 10000) + 
				if(~ut.LeapYear(year), 
					MAP( day > 334 => 1200 + (day - 334),
				day > 304 => 1100 + (day - 304),
				day > 273 => 1000 + (day - 273),
				day > 243 => 0900 + (day - 243),
				day > 212 => 0800 + (day - 212),
				day > 181 => 0700 + (day - 181),
				day > 151 => 0600 + (day - 151),
				day > 120 => 0500 + (day - 120),
				day > 90  => 0400 + (day - 90),
				day > 59  => 0300 + (day - 59),
				day > 31  => 0200 + (day - 31),
				day > 0   => 0100 + day, 0),
					MAP( day > 335  => 1200 + (day - 335),
				day > 305 => 1100 + (day - 305),
				day > 274 => 1000 + (day - 274),
				day > 244 => 0900 + (day - 244),
				day > 213 => 0800 + (day - 213),
				day > 182 => 0700 + (day - 182),
				day > 152 => 0600 + (day - 152),
				day > 121 => 0500 + (day - 121),
				day > 91  => 0400 + (day - 91),
				day > 60  => 0300 + (day - 60),
				day > 31  => 0200 + (day - 31),
				day > 0   => 0100 + day, 0))
			,8, 1);