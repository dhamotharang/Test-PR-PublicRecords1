// This attribute's function is to map a message code to a textual description (similar to ErrorCodes)
import ut;
export varstring MapMessageCodes(integer c) :=
	CASE(c,	
			ut.constants_MessageCodes.PHONE_FALLBACK	=>			'Reverse phone lookup reverted to 7-digit match within State',
	    ut.constants_MessageCodes.TOO_MANY_RECORDS =>		'Too many records found',
			ut.constants_MessageCodes.TOO_MANY_ADDRESSES =>	'Too many addresses found',
			ut.constants_MessageCodes.REMOVE_MNAME =>				'Removing Middle Name will produce additional results',
			ut.constants_MessageCodes.SUBJECT_NOT_UNIQUE=>		'Subject cannot be uniquely identified. Please include additional identifying information such as DOB, SSN, or Address.',
      ut.constants_MessageCodes.EMAIL_NOT_UNIQUE=>     'Email address cannot be uniquely identified. Please include additional information such as full name, SSN, or Address.',
      ut.constants_MessageCodes.MAX_INPUT_EXCEEDED=>   'Maximum number of requests exceeded',
      ut.constants_MessageCodes.IMPOSTERS_EXCEED_LIMIT=>'Too many individuals associated with SSN',			
			ut.constants_MessageCodes.RTV_GATEWAY_MISSING	=>  'RealTime Vehicle Gateway cannot process request',
      ut.constants_MessageCodes.POLK_ERROR					=>  'Generic RealTime Vehicle Gateway Error',
      ut.constants_MessageCodes.GATEWAY_URL_MISSING =>  'Gateway URL missing',
      ut.constants_MessageCodes.TRYSSN4						  =>  'Hint: To broaden your search request, you may want to attempt a search by Last Name and last four digits of the SSN.',
      ut.constants_MessageCodes.FDN_GC_ID						=>  'FDN_GlobalCompanyId is not entered.',
      ut.constants_MessageCodes.FDN_PRODUCT_CODE		=>  'FDN_Product Code is not entered.',
      ut.constants_MessageCodes.FDN_INDUSTRY_TYPE		=>  'FDN_Industry Type is not entered.',
			ut.constants_MessageCodes.FDN_NO_INPUT_PROVIDED		=>  'One of the following should be provided: UniqueID, SSN, Phone Number, Address, (SeleId & OrgId & UltId), AppendedProviderId, LNPID, TIN, NPI, EmailAddress, IpAddress, DeviceId or ProfessionalId.',
		  ut.constants_MessageCodes.DELTABASE_NOT_AVAILABLE  => 'Deltabase Not Available.',
      ut.constants_MessageCodes.DEMO_SEARCH_TOOL_INVAILD_NUM_BIZ        => 'Demo Search Tool: The count for the Number of Buinsess Executives must be greater than zero when a combined Person-Business Search is Requested.',
      ut.constants_MessageCodes.DEMO_SEARCH_TOOL_INVAILD_SEARCH_OPTIONS => 'Demo Search Tool: Invalid search. You can not select FCRA and DTC for the same search.',
      ut.constants_MessageCodes.DEMO_SEARCH_TOOL_INVAILD_BIZ_COUNT      => 'Demo Search Tool: Invalid search. You must enter a count of at least 2 when searching for multiple businesses at an address.',
			ut.constants_MessageCodes.DEMO_SEARCH_TOOL_INVAILD_SEARCH_TYPE    => 'Demo Search Tool: You must enter a valid search type: Person, business, Multi Businesses at an address.',
      ut.constants_MessageCodes.DEMO_SEARCH_TOOL_INVAILD_SEARCH_INPUT   => 'Demo Search Tool: You must enter a valid integer for the counts.',
      ut.constants_MessageCodes.FCRA_CASE_NUMBER_MIN_INPUT_NOT_MET      => 'FCRA Case Num Search: Minimum search input not met. Please enter case number + SSN  OR case number + first name + last name + state.',
      ut.constants_MessageCodes.FCRA_CASE_NUM_MORE_THAN_1_REC_RETURNED  => 'FCRA Case Num Search: Not enough information. Please enter more input fields to narrow to one person.',  
      '');