// This module is all about the BIP2 header layout.  It's meant to be referenced by
// BIPV2.CommonBase and nothing else, but we don't really have a way to enforce that.
IMPORT BIPV2, BIPV2_Company_Names, AID;
EXPORT CommonBase_mod := MODULE
	
	// Received from the data team
	l_as_linking := BIPV2.Layout_Business_Linking_Full;
	
	// The header layout is largely derived from the data team layout
	EXPORT Layout_Derived := RECORD
		unsigned6	rcid;
		string2		source;
		string9		ingest_status; // Old, Unchanged, Updated, or New
		
		// BIPV2.IDlayouts.l_header_ids (plus LGID3)
		unsigned6	DotID;
		unsigned6	EmpID;
		unsigned6	POWID;
		unsigned6	ProxID;
		unsigned6	SELEID;
		unsigned6	LGID3;
		unsigned6	OrgID;
		unsigned6	UltID;
