import BBB2,MDR;

export BBB_AsMasters := module(Interface_AsMasters.Unlinked.Default)

	shared base_member := BBB2.Files(,true).Base.Member.QA;
	shared base_nonmember := BBB2.Files(,true).Base.NonMember.QA;
	
	shared filtered_member := base_member(company_name = '' and bbb_id != '');
	shared filtered_nonmember := base_nonmember(company_name = '' and bbb_id != '');
	
	export dataset(Layout_Linking.Unlinked) As_Linking_Master := function
	
		extract_1 := normalize(filtered_member,
			if(left.v_city_name != left.p_city_name,2,1),
			transform(Layout_Linking.Unlinked,
				self.source := MDR.sourceTools.src_BBB_Member,
				self.source_docid := left.bbb_id,
				self.source_party := '',
				self.date_first_seen := (unsigned4)left.date_first_seen,
				self.date_last_seen := (unsigned4)left.date_last_seen,
				self.company_name := stringlib.StringToUpperCase(left.company_name),
				self.company_name_type := Constants.Company_Name_Types.UNKNOWN,
				self.address_type := Constants.Address_Types.UNKNOWN,
				self.phone_type := Constants.Phone_Types.UNKNOWN,
				self.aid := 0,
				self.prim_range := left.prim_range,
				self.predir := left.predir,
				self.prim_name := left.prim_name,
				self.addr_suffix := left.addr_suffix,
				self.postdir := left.postdir,
				self.unit_desig := left.unit_desig,
				self.sec_range := left.sec_range,
				self.city_name := choose(counter,
					left.p_city_name,
					left.v_city_name),
				self.state := left.st,
				self.zip := left.zip,
				self.zip4 := left.zip4,
				self.county_fips := left.fips_county,
				self.msa := left.msa,
				self.phone := left.phone10,
				self.fein := '',
				self.url := '',
				self.duns := '',
				self.experian := '',
				self.zoom := '',
				self.incorp_state := '',
				self.incorp_number := ''));
		
		extract_2 := normalize(filtered_member(member_title != company_name),
			if(left.v_city_name != left.p_city_name,2,1),
			transform(Layout_Linking.Unlinked,
				self.source := MDR.sourceTools.src_BBB_Member,
				self.source_docid := left.bbb_id,
				self.source_party := '',
				self.date_first_seen := (unsigned4)left.date_first_seen,
				self.date_last_seen := (unsigned4)left.date_last_seen,
				self.company_name := stringlib.StringToUpperCase(left.member_title),
				self.company_name_type := Constants.Company_Name_Types.UNKNOWN,
				self.address_type := Constants.Address_Types.UNKNOWN,
				self.phone_type := Constants.Phone_Types.UNKNOWN,
				self.aid := 0,
				self.prim_range := left.prim_range,
				self.predir := left.predir,
				self.prim_name := left.prim_name,
				self.addr_suffix := left.addr_suffix,
				self.postdir := left.postdir,
				self.unit_desig := left.unit_desig,
				self.sec_range := left.sec_range,
				self.city_name := choose(counter,
					left.p_city_name,
					left.v_city_name),
				self.state := left.st,
				self.zip := left.zip,
				self.zip4 := left.zip4,
				self.county_fips := left.fips_county,
				self.msa := left.msa,
				self.phone := left.phone10,
				self.fein := '',
				self.url := '',
				self.duns := '',
				self.experian := '',
				self.zoom := '',
				self.incorp_state := '',
				self.incorp_number := ''));
		
		extract_3 := normalize(filtered_nonmember,
			if(left.v_city_name != left.p_city_name,2,1),
			transform(Layout_Linking.Unlinked,
				self.source := MDR.sourceTools.src_BBB_Non_Member,
				self.source_docid := left.bbb_id,
				self.source_party := '',
				self.date_first_seen := (unsigned4)left.date_first_seen,
				self.date_last_seen := (unsigned4)left.date_last_seen,
				self.company_name := stringlib.StringToUpperCase(left.company_name),
				self.company_name_type := Constants.Company_Name_Types.UNKNOWN,
				self.address_type := Constants.Address_Types.UNKNOWN,
				self.phone_type := Constants.Phone_Types.UNKNOWN,
				self.aid := 0,
				self.prim_range := left.prim_range,
				self.predir := left.predir,
				self.prim_name := left.prim_name,
				self.addr_suffix := left.addr_suffix,
				self.postdir := left.postdir,
				self.unit_desig := left.unit_desig,
				self.sec_range := left.sec_range,
				self.city_name := choose(counter,
					left.p_city_name,
					left.v_city_name),
				self.state := left.st,
				self.zip := left.zip,
				self.zip4 := left.zip4,
				self.county_fips := left.fips_county,
				self.msa := left.msa,
				self.phone := left.phone10,
				self.fein := '',
				self.url := '',
				self.duns := '',
				self.experian := '',
				self.zoom := '',
				self.incorp_state := '',
				self.incorp_number := ''));
		
		extract_4 := normalize(filtered_nonmember(non_member_title != company_name),
			if(left.v_city_name != left.p_city_name,2,1),
			transform(Layout_Linking.Unlinked,
				self.source := MDR.sourceTools.src_BBB_Non_Member,
				self.source_docid := left.bbb_id,
				self.source_party := '',
				self.date_first_seen := (unsigned4)left.date_first_seen,
				self.date_last_seen := (unsigned4)left.date_last_seen,
				self.company_name := stringlib.StringToUpperCase(left.non_member_title),
				self.company_name_type := Constants.Company_Name_Types.UNKNOWN,
				self.address_type := Constants.Address_Types.UNKNOWN,
				self.phone_type := Constants.Phone_Types.UNKNOWN,
				self.aid := 0,
				self.prim_range := left.prim_range,
				self.predir := left.predir,
				self.prim_name := left.prim_name,
				self.addr_suffix := left.addr_suffix,
				self.postdir := left.postdir,
				self.unit_desig := left.unit_desig,
				self.sec_range := left.sec_range,
				self.city_name := choose(counter,
					left.p_city_name,
					left.v_city_name),
				self.state := left.st,
				self.zip := left.zip,
				self.zip4 := left.zip4,
				self.county_fips := left.fips_county,
				self.msa := left.msa,
				self.phone := left.phone10,
				self.fein := '',
				self.url := '',
				self.duns := '',
				self.experian := '',
				self.zoom := '',
				self.incorp_state := '',
				self.incorp_number := ''));
		
		return extract_1 + extract_2 + extract_3 + extract_4;
	
	end;
	
	export dataset(Layout_Industry.Unlinked) As_Industry_Master := function
	
		extract_1 := project(filtered_member,
			transform(Layout_Industry.Unlinked,
				self.source := MDR.sourceTools.src_BBB_Member,
				self.source_docid := left.bbb_id,
				self.source_party := '',
				self.industry_description := left.member_category,
				self := []));
		
		extract_2 := project(filtered_nonmember,
			transform(Layout_Industry.Unlinked,
				self.source := MDR.sourceTools.src_BBB_Non_Member,
				self.source_docid := left.bbb_id,
				self.source_party := '',
				self.industry_description := left.non_member_category,
				self := []));
		
		return extract_1 + extract_2;
	
	end;
	
end;
