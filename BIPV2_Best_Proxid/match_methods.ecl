	MAP(L = R => SALT30.MatchCode.ExactMatch,
		SALT30.WithinEditN(L,R,1, 0) => SALT30.MatchCode.EditDistanceMatch,
		SALT30.MatchCode.NoMatch),
	MAP(L = R => SALT30.MatchCode.ExactMatch, SALT30.MatchCode.NoMatch)
);
