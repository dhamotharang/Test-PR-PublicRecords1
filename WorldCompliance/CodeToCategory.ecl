EXPORT string CodeToCategory(integer code) := Case(code,
1	=> 'PEP',
3	=> 'Sanction List',
10	=> 'Associated Entity',
14	=> 'Enforcement',
23	=> 'Investigation',
25	=> 'SOE',
29	=> 'Adverse Media',
30	=> 'Influential Person',
31	=> 'Company of Interest',
32	=> 'Excluded Party',
33	=> 'SOE',
34	=> 'End-Use Controls',
FAIL(integer, 'Unexpected category: ' + (string)code));