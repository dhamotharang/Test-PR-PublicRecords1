IMPORT ut, carrierDiscovery;
EXPORT Files := MODULE
		EXPORT string FOREIGN_PREFIX         := '~';

		EXPORT SPRAYED_PREFIX_NAME           := FOREIGN_PREFIX + 'THOR::SPRAY::LEAD';
		EXPORT BASE_PREFIX_NAME              := FOREIGN_PREFIX + 'THOR::BASE::LEAD';
		EXPORT KEY_PREFIX_NAME               := FOREIGN_PREFIX + 'THOR::KEY::LEAD';

		EXPORT SUFFIX_NAME_DELTA_REPORT      := BuildConstants().lead_delta_report;
		EXPORT FILE_BASE_DELTA_REPORT_SF     := BASE_PREFIX_NAME  + '::QA::' + SUFFIX_NAME_DELTA_REPORT;
		EXPORT FILE_KEY_DELTA_REPORT         := KEY_PREFIX_NAME + '::' + SUFFIX_NAME_DELTA_REPORT;
		EXPORT FILE_KEY_DELTA_REPORT_SF      := FILE_KEY_DELTA_REPORT + '_QA';
		EXPORT DS_BASE_DELTA_REPORT					 := DATASET(FILE_BASE_DELTA_REPORT_SF, BuildLayouts.Layout_Delta_Rec, THOR, OPT);
		
		EXPORT SUFFIX_NAME_LDL_FILE          := BuildConstants().lead_ldl_file;
		EXPORT SUFFIX_NAME_LDL_CUST_ZIP_St   := BuildConstants().lead_ldl_cust_zip_st;
		EXPORT FILE_BASE_LDL_FILE_SF         := BASE_PREFIX_NAME  + '::QA::' + SUFFIX_NAME_LDL_FILE;
		EXPORT FILE_KEY_LDL_CUST_ZIP_St      := KEY_PREFIX_NAME + '::' + SUFFIX_NAME_LDL_CUST_ZIP_ST;
		EXPORT FILE_KEY_LDL_CUST_ZIP_ST_SF   := FILE_KEY_LDL_CUST_ZIP_ST + '_QA';
		EXPORT DS_BASE_LDL_FILE						   := DATASET(FILE_BASE_LDL_FILE_SF, BuildLayouts.Layout_ldl_Rec, THOR, OPT);
		
		EXPORT SUFFIX_NAME_DNS_FILE          := BuildConstants().lead_dns_file;
		EXPORT SUFFIX_NAME_DNS_CUST_IDL      := BuildConstants().lead_dns_cust_idl;
		EXPORT FILE_BASE_DNS_FILE_SF         := BASE_PREFIX_NAME  + '::QA::' + SUFFIX_NAME_DNS_FILE;
		EXPORT FILE_KEY_DNS_CUST_IDL         := KEY_PREFIX_NAME + '::' + SUFFIX_NAME_DNS_CUST_IDL;
		EXPORT FILE_KEY_DNS_CUST_IDL_SF      := FILE_KEY_DNS_CUST_IDL+ '_QA';
		EXPORT DS_BASE_DNS_FILE						   := DATASET(FILE_BASE_DNS_FILE_SF, BuildLayouts.Layout_dns_Rec, THOR, OPT);
END;