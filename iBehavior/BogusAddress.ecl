export BogusAddress(string in_value) := function

exception_set := ['CCC','RRRT'];

boolean in_value_all_consonants := trim(stringlib.stringfilter(in_value,'BCDFGHJKLMNPQRSTVWXZ'))=trim(in_value);
// integer prim_name_length        := length(trim(in_value));
                        
boolean ret_value := REGEXFIND('(ANYWHERE|APP 12-24-99|^AS OF |BAD ADDRESS|BAD NUMBER|^BRANCH CLOSED|BROKER OF RECORD|^BY APPL|CANCEL LICENSE|^CANCEL$|CHANGE ON FILE|CHNGED BRKR ACCRDING TO|CODE ENFORCEMENT OFFICER|CORRECTED BROKER OF REC|CORRECTED NAME NO|'+
															 'DECEASE|DELETE |^DIED$|DISCIPLINARY|DO NOT USE|DUMMY RECORD|DUMMY TEST|^DUPLICATE$|DUPLICATE |ENTERED IN ERROR|ERROR ISSUED|EXPD. |'+
															 'INACTIVE|INCORRECT|IN ERROR|INPUT ERROR|INVALID NUMBER|INVALID|ISSUE|ISSUED IN ERROR|LICENSE NO|LICENSE RENEWAL|LICENSEE|LICENSE NUMBER|'+
															 'LICENSE TO LEND|MAIL RETURN|MARRIED|^MISSING|MOVED TO|^NA$|^N/A$|N/AVAIL|NEED NEW ADDRESS|NEW BRKR & LIC|NO ADDRESS|NO BROKER OF REC|NO LONGER EMPLOYED|NO VALID$|^NONE$|NONE GIVEN|NON INDICATED|NONE LISTED|^NONE |NOT APPLICABLE|NOT AVAILABLE|'+
															 'NOT CURRENTLY|NOT EMPLOYED|NOT GIVEN|NOT GVN|NOT ON FILE|NOT RENEWED|N/AVAIL|^OFFICE CLOSED|OFFICE CURRENT IS TEMPORARY|OUT OF BUSINESS|PER COLBY|PRVT PRIVATE|^REF |REQUESTED TO BE INACTIVED|RENEWING|REQUESTED|'+
															 'RES BROK OF REC |RETURNED MAIL|RETURN TO SENDER|^SAME$|SAME AS |SEE COMMENT|SEE STR |SEE ID |^TBD|TEMP EXP |TEMP LIC|^TEMPORARY|TERMINATED|TERMNATED|TEST DH IA|TEST LENDER|TEST RECORD|^TEST$|'+
															 'TO BE DETERMINED|TO 1/9/02|UNABLE TO LOCATE LENDER|^USE |UNEMPLOYED|^UNKNOWN|^VOID |WITHDRAWN|^XXX)',trim(in_value))

or
(
    // prim_name_length > 2 
	in_value_all_consonants=true
and in_value[1]=in_value[2] and in_value[2]=in_value[3]
and in_value not in exception_set
);

return ret_value;

end;                        
