IMPORT ut;

EXPORT Layout_Business_Contact_Plus TRA_Merge_Employees_Business(Layout_Business_Contact_Plus L, Layout_Business_Contact_Plus R) := TRANSFORM
SELF.source := IF(L.source = '', R.source, L.source);
