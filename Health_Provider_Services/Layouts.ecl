		STRING15    prim_name			;	// [13..40]
		STRING5     sec_range			;	// [57..64]
		STRING10    v_city_name		; // [90..114]
		STRING2     st						;	// [115..116]
		STRING5     zip						;	// [117..121]
		STRING6			UPIN;
