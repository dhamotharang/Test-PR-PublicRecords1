OPTIONS:-gh
MODULE:Scrubs_Certegy
FILENAME:Certegy
NAMESCOPE:raw_file 
// Uncomment up to NINES for internal or external adl
// IDFIELD:EXISTS:<NameOfIDField>
// RIDFIELD:<NameOfRidField>
// RECORDS:<NumberOfRecordsInDataFile>
// POPULATION:<ExpectedNumberOfEntitiesInDataFile>
// NINES:<Precision required 3 = 99.9%, 2 = 99% etc>
// Uncomment Process if doing external adl
// PROCESS:<ProcessName>
// FIELDTYPE statements can be used to clean up (or check the cleaning) of individual fields
// BESTTYPE statements declare methods of generating the best value for a given cluster; this can also improve linking
// FUZZY can be used to create new types of FUZZY linking
// Remember to generate specificities and update the 0,0 placeholders below before running any sort of linking.
// If the actual specificity for a field is <1, round it up to 1 rather than down to 0.  If your cluster is running
// a shared repository, calling SALTTOOLS30.mac_Patch_SPC from the bottom of BWR_Specificities may be a convenience.

FIELDTYPE:invalid_dl_state:CUSTOM(Scrubs_Certegy.functions.fn_verify_state>0)
FIELDTYPE:invalid_dl_num:ALLOW(*0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ)
FIELDTYPE:invalid_ssn:CUSTOM(Scrubs_Certegy.functions.fn_valid_ssn>0)
FIELDTYPE:invalid_dl_issue_dte:CUSTOM(Scrubs_Certegy.functions.fn_past_date>0)
FIELDTYPE:invalid_dl_expire_dte:CUSTOM(Scrubs_Certegy.functions.fn_general_date>0)
FIELDTYPE:invalid_house_bldg_num:ALLOW( -/0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ)
FIELDTYPE:invalid_street_suffix:ALLOW(ABCDEFGHIJKLMNOPQRSTUVWXYZ)
FIELDTYPE:invalid_apt_num:ALLOW( /-0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ)
FIELDTYPE:invalid_unit_desc:ALLOW(#ABCDEFGHIJKLMNOPQRSTUVWXYZ)
FIELDTYPE:invalid_street_post_dir:ALLOW(ENSW)
FIELDTYPE:invalid_street_pre_dir:ALLOW(ENSW)
FIELDTYPE:invalid_state:CUSTOM(Scrubs_Certegy.functions.fn_verify_state>0)
FIELDTYPE:invalid_zip:CUSTOM(Scrubs_Certegy.functions.fn_verify_zip5>0)
FIELDTYPE:invalid_zip4:CUSTOM(Scrubs_Certegy.functions.fn_verify_zip4>0)
FIELDTYPE:invalid_dob:CUSTOM(Scrubs_Certegy.functions.fn_past_date>0)
FIELDTYPE:invalid_deceased_dte:CUSTOM(Scrubs_Certegy.functions.fn_deceased_dte>0,deceased_dte,dob)
FIELDTYPE:invalid_home_tel_area:CUSTOM(Scrubs_Certegy.functions.fn_CleanPhone>0,home_tel_area,home_tel_num)
FIELDTYPE:invalid_home_tel_num:CUSTOM(Scrubs_Certegy.functions.fn_CleanPhone>0,home_tel_area,home_tel_num)
FIELDTYPE:invalid_work_tel_area:CUSTOM(Scrubs_Certegy.functions.fn_CleanPhone>0,work_tel_area,work_tel_num)
FIELDTYPE:invalid_work_tel_num:CUSTOM(Scrubs_Certegy.functions.fn_CleanPhone>0,work_tel_area,work_tel_num)
FIELDTYPE:invalid_work_tel_ext:CUSTOM(Scrubs_Certegy.functions.fn_tel_ext>0,work_tel_ext)
FIELDTYPE:invalid_upd_dte_time:CUSTOM(Scrubs_Certegy.functions.fn_past_date>0)
FIELDTYPE:invalid_first_name:CUSTOM(Scrubs_Certegy.functions.fn_chk_blank_names>0,first_name,mid_name,last_name)
FIELDTYPE:invalid_mid_name:CUSTOM(Scrubs_Certegy.functions.fn_chk_blank_names>0,first_name,mid_name,last_name)
FIELDTYPE:invalid_last_name:CUSTOM(Scrubs_Certegy.functions.fn_chk_blank_names>0,first_name,mid_name,last_name)
FIELDTYPE:invalid_gen_delivery:CUSTOM(Scrubs_Certegy.functions.fn_gen_del>0)
FIELDTYPE:invalid_street_name:ALLOW( .-/0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ)
FIELDTYPE:invalid_city:ALLOW( ABCDEFGHIJKLMNOPQRSTUVWXYZ)

FIELD:dl_state:TYPE(STRING2):LIKE(invalid_dl_state):0,0
FIELD:dl_num:TYPE(STRING24):LIKE(invalid_dl_num):0,0
FIELD:ssn:TYPE(STRING9):LIKE(invalid_ssn):0,0
FIELD:dl_issue_dte:TYPE(STRING10):LIKE(invalid_dl_issue_dte):0,0
FIELD:dl_expire_dte:TYPE(STRING10):LIKE(invalid_dl_expire_dte):0,0
FIELD:house_bldg_num:TYPE(STRING10):LIKE(invalid_house_bldg_num):0,0
FIELD:street_suffix:TYPE(STRING4):LIKE(invalid_street_suffix):0,0
FIELD:apt_num:TYPE(STRING10):LIKE(invalid_apt_num):0,0
FIELD:unit_desc:TYPE(STRING4):LIKE(invalid_unit_desc):0,0
FIELD:street_post_dir:TYPE(STRING2):LIKE(invalid_street_post_dir):0,0
FIELD:street_pre_dir:TYPE(STRING2):LIKE(invalid_street_pre_dir):0,0
FIELD:state:TYPE(STRING2):LIKE(invalid_state):0,0
FIELD:zip:TYPE(STRING5):LIKE(invalid_zip):0,0
FIELD:zip4:TYPE(STRING4):LIKE(invalid_zip4):0,0
FIELD:dob:TYPE(STRING10):LIKE(invalid_dob):0,0
FIELD:deceased_dte:TYPE(STRING10):LIKE(invalid_deceased_dte):0,0
FIELD:home_tel_area:TYPE(STRING3):LIKE(invalid_home_tel_area):0,0
FIELD:home_tel_num:TYPE(STRING7):LIKE(invalid_home_tel_num):0,0
FIELD:work_tel_area:TYPE(STRING3):LIKE(invalid_work_tel_area):0,0
FIELD:work_tel_num:TYPE(STRING7):LIKE(invalid_work_tel_num):0,0
FIELD:work_tel_ext:TYPE(STRING5):LIKE(invalid_work_tel_ext):0,0
FIELD:upd_dte_time:TYPE(STRING26):LIKE(invalid_upd_dte_time):0,0
FIELD:first_name:TYPE(STRING20):LIKE(invalid_first_name):0,0
FIELD:mid_name:TYPE(STRING20):LIKE(invalid_mid_name):0,0
FIELD:last_name:TYPE(STRING35):LIKE(invalid_last_name):0,0
FIELD:gen_delivery:TYPE(STRING20):LIKE(invalid_gen_delivery):0,0
FIELD:street_name:TYPE(STRING35):LIKE(invalid_street_name):0,0
FIELD:city:TYPE(STRING25):LIKE(invalid_city):0,0
FIELD:foreign_cntry:TYPE(STRING25):0,0 
// CONCEPT statements should be used to group together interellated fields; such as address
// RELATIONSHIP is used to find non-obvious relationships between the clusters
// SOURCEFIELD is used if a field of the file denotes a source of the records in that file
// LINKPATH is used to define access paths for external linking
