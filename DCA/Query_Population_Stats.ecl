	integer4 geo_longA_count                          := sum(group, if(dca_file.geo_longA                       <> '',1,0));
	integer4 msaA_count                               := sum(group, if(dca_file.msaA                            <> '',1,0));
	integer4 geo_blkA_count                           := sum(group, if(dca_file.geo_blkA                        <> '',1,0));
	integer4 geo_matchA_count                         := sum(group, if(dca_file.geo_matchA                      <> '',1,0));
	integer4 err_statA_count                          := sum(group, if(dca_file.err_statA                       <> '',1,0));
	integer4 lf_count                                 := sum(group, if(dca_file.lf                              <> '',1,0));
	integer4 total                                    := count(group);
end;
dca_file_stat := table(dca_file, Layout_dca_file_stat, few);
output_dca_file_stat := output(dca_file_stat, named('dca_field_population_stats'), all);


export Query_Population_Stats := output_dca_file_stat;