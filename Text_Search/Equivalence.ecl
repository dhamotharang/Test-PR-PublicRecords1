	SHARED BOOLEAN isCurrent(Types.NominalSuffix dictVer) := dictVer in [0, Constants.CurrDictVer];

	EXPORT DATASET(Entry) get(STRING kwd, Types.NominalSuffix dictVer = 0) := FUNCTION
		STRING tmpStr := StringLib.StringToUpperCase(kwd);

		RETURN SORTED(myd, wd)(wd = tmpStr);
	END;

	EXPORT DATASET(Entry) getDirectional(STRING kwd) := FUNCTION
		STRING tmpStr := StringLib.StringToUpperCase(kwd);

		RETURN SORTED(drctd, wd)(wd = tmpStr);
	END;
	
	EXPORT STRING getEquiv(STRING kwd, Types.NominalSuffix dictVer = 0) := FUNCTION
		STRING tmpStr := StringLib.StringToUpperCase(kwd);

		RETURN (myd(wd = tmpStr)[1].wds[1]);
	END;
	
	EXPORT STRING getDirRepl(Entry input) := StringLib.StringFilterOut(input.wd, '*');

	EXPORT BOOLEAN kwdPunctMatch(STRING kwdPunct, Types.NominalSuffix dictVer) := FUNCTION
		STRING tmpStr := StringLib.StringToUpperCase(kwdPunct);

		RETURN IF(isCurrent(dictVer), tmpStr IN KwdWithPunctsCurrent,
																	tmpStr IN KwdWithPunctsOld);
	END;
	
	SHARED ValidateRec := RECORD
				WdStr	word;
	END;
		
	SHARED ValidateRec2 := RECORD
				WdStr	 word;
				WdStr  parentWord;
	END;
	
	SHARED reportlay := RECORD
			DATASET(Entry)					missingMYDrecs;
			DATASET(Entry)					missingEquivrecs;
			DATASET(ValidateRec2)		missingPrecInEquivSet;
			DATASET(ValidateRec)		missingEquivSetPhrases;
	END;
	
	/*
	The function ValidateEquivAttributes is to be used to ensure that every equivalent in myd attribute
	has a matching equivalent within the MultiWrdEquivalents or the SingleWrdEquivalents and vice versa.
  If an equivalent exists in the MultiWrdEquivalents or SingleWrdEquivalents but not in the myd, then that
  equivalent will be listed under the MissingMYDrecs field in the output. If an equivalent exists within the
  myd but not in either the MultiWrdEquivalents or SingleWrdEquivalents then the equivalent will be listed under
  the MissingMultiSingleEquivrecs field in the output. The function also validates the EquivSet to ensure every 
  preceding word in the precWds Set has a match in the EquivSet wd; additionally it will construct all possible
  phrases from the wd and precWds set and ensure they all exist in the myd equivalent.
  To use, simply call this function via a builder window. I.E.
  Text_Search.Equivalence.ValidateEquivAttributes;
 */
	EXPORT DATASET(reportlay) ValidateEquivAttributes := FUNCTION
										
		MultiEquivDs := PROJECT(DATASET(MultiWrdEquivalents,ValidateRec),TRANSFORM(Entry,SELF.wd := LEFT.word, SELF.equivType := 'M', SELF := []));
		SingleEquivDs := PROJECT(DATASET(SingleWrdEquivalents,ValidateRec),TRANSFORM(Entry,SELF.wd := LEFT.word, SELF.equivType := 'S', SELF := []));
		KwdPunctEquivDs := PROJECT(DATASET(KwdWithPuncts,ValidateRec),TRANSFORM(Entry,SELF.wd := LEFT.word, SELF.equivType := 'S', SELF := []));
		missingInEquiv := JOIN (myd,MultiEquivDs+SingleEquivDs+SingleEquivDs+KwdPunctEquivDs,
															LEFT.wd = RIGHT.wd AND LEFT.equivType = RIGHT.equivType, LEFT ONLY);
		missingInMyd := JOIN (DEDUP(MultiEquivDs+SingleEquivDs+KwdPunctEquivDs,wd,ALL),myd,
															LEFT.wd = RIGHT.wd AND LEFT.equivType = RIGHT.equivType, LEFT ONLY);
	
		precWds := DEDUP(SORT(NORMALIZE(EquivSet,COUNT(LEFT.precWds),TRANSFORM(ValidateRec2,SELF.word := LEFT.precWds[COUNTER],
																																							 SELF.parentWord := LEFT.wd)),word),word);
		missingPrecInEquivSet := JOIN(precWds,EquivSet, LEFT.word = RIGHT.wd,TRANSFORM(ValidateRec2,SELF := LEFT), LEFT ONLY);
		equivSetPhrases := NORMALIZE(EquivSet,COUNT(LEFT.precWds),TRANSFORM(ValidateRec,SELF.word := LEFT.precWds[COUNTER] + ' ' + LEFT.wd));
		missingEquivSetPhrases := JOIN(equivSetPhrases,myd,LEFT.word = RIGHT.wd, TRANSFORM(ValidateRec,SELF := LEFT), LEFT ONLY);
		
		outrec := DATASET([{missingInEquiv,missingInMyd,missingPrecInEquivSet,missingEquivSetPhrases}],reportlay);
		RETURN(outrec);
	END;
	
END;