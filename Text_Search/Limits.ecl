export Limits := MODULE
	EXPORT INTEGER Max_Join			:= 200000;
	EXPORT INTEGER Max_Remote		:= 2000;
	EXPORT INTEGER Max_Read			:= 10000000;
	EXPORT INTEGER Max_Resolve	:= 1000;
	EXPORT INTEGER Max_Merge		:= 10000000;
	EXPORT INTEGER Max_Wild			:= 400;															// Need to stay within 64K
	EXPORT INTEGER Max_Wild_Defer := Max_Wild;	                    // defer any above this value
	EXPORT INTEGER Max_LocalWild:= 1000000;
	EXPORT INTEGER Max_Equiv		:= Max_Wild;
	EXPORT INTEGER Max_Lookup		:= 100;
	EXPORT INTEGER Max_Parts		:= 500;
	EXPORT INTEGER Max_DocHits	:= 10000;	                        	// hits per document limit
	EXPORT INTEGER Max_Terms		:= 100;
	EXPORT INTEGER Max_Segments := 20000;
	EXPORT INTEGER Max_SegHits	:= 1000;
	EXPORT INTEGER Max_Search		:= 100000;
	EXPORT INTEGER Max_Focus    := 3000;
	EXPORT INTEGER Max_Hits			:= 250000;
	EXPORT INTEGER Max_1_Search := 100000;
	EXPORT INTEGER Max_Appended_Terms := 2;													// Additional terms for focus and alerts

	// Message
	EXPORT INTEGER MessageBase	:= 12300;
	EXPORT STRING	 Read_Msg 		:= 'Read limit exceeded';
	EXPORT INTEGER Read_Code		:= MessageBase + 1;
	EXPORT Read_Fail						:= FAIL(Read_Code, Read_Msg);
	EXPORT STRING	 Join_Msg			:= 'Join limit exceeded';
	EXPORT INTEGER Join_Code		:= Messagebase + 2;
	EXPORT Join_Fail						:= FAIL(Join_Code, Join_Msg);
	EXPORT STRING  Remote_Msg		:= 'Node results limit exceeded';
	EXPORT INTEGER Remote_Code	:= MessageBase + 3;
	EXPORT Remote_Fail					:= FAIL(Remote_code, Remote_Msg);
	EXPORT STRING  Resolve_Msg  := 'Resolve limit exceeded';
	EXPORT INTEGER Resolve_Code	:= MessageBase + 4;
	EXPORT Resolve_Fail					:= FAIL(Resolve_Code, Resolve_Msg);
	EXPORT STRING	 Merge_Msg		:= 'Limit on merge exceeded';
	EXPORT INTEGER Merge_Code		:= MessageBase + 5;
	EXPORT Merge_Fail						:= FAIL(Merge_Code, Merge_Msg);
	EXPORT STRING	 Wild_Msg			:= 'Limit on the wild card match';
	EXPORT INTEGER Wild_Code		:= MessageBase + 6;
	EXPORT Wild_Fail						:= FAIL(Wild_Code, Wild_Msg);
	EXPORT STRING	 AnsLim_Msg		:= 'Answer Limit exceeded';
	EXPORT INTEGER AnsLim_Code	:= MessageBase + 7;
	EXPORT AnsLim_Fail					:= FAIL(AnsLim_Code, AnsLim_Msg);	
	EXPORT STRING Lookup_Msg		:= 'Special data lookup returned too many values';
	EXPORT INTEGER Lookup_Code	:= MessageBase + 8;
	EXPORT STRING Parts_Msg			:= 'Number of partitions too great';
	EXPORT INTEGER Parts_Code		:= MessageBase + 9;
	EXPORT STRING Segments_Msg  := 'Too many segments defined';
	EXPORT INTEGER Segments_Code:= MessageBase + 10;
	EXPORT STRING Breadth_Msg   := 'Implementation problem for Breadth Merge';
	EXPORT INTEGER Breadth_Code := MessageBase + 11;
	EXPORT STRING	 TooManyTerms_Msg		:= 'Too many search terms';
	EXPORT INTEGER TooManyTerms_Code	:= MessageBase + 12;
	EXPORT STRING	 ParseErr_Msg		:= 'Parse error: ';
	EXPORT INTEGER ParseErr_Code	:= MessageBase + 13;
	EXPORT STRING  FocusLim_Msg  := 'Focus Limit Exceeded';
	EXPORT INTEGER FocusLim_Code := MessageBase+14;
	EXPORT STRING  DictNoGood_Msg		:= 'Incompatible dictionary';
	EXPORT INTEGER DictNoGood_Code	:= MessageBase+15;
	EXPORT STRING TooManyEquiv_Msg	:= 'Too many equivalents for term';
	EXPORT INTEGER TooManyEquiv_Code:= MessageBase+16;
	EXPORT STRING CommonSingle_Msg	:= 'Only common single term given';
	EXPORT INTEGER CommonSingle_Code:= MessageBase+17;
END;