  // batch request input layout (most complete)
  export batch := RECORD
    nco_in;
    batch_in;
  end;

  // "raw" monitor: used for linking back to the originals the results of monitoring;
  // same layout is used to archive all batch requests (monitor state can be restored from this file, if needed)
  export batch_raw := RECORD
    layouts.wid;
    nco_in;
    batch_address_phone;
  end;

  // this layout will be used when joining monitor results back with raw records: all fields in
  // batch portion should be blanked, except "new found" phones and/or addresses
  export monitor_update := RECORD
    layouts.id;
    batch_address_phone;
    scores;
  end;

  // output from monitoring process; this will be eventually send back to a client
  export monitor_result := RECORD
    nco_in.nco_server;
    nco_in.work_source_id;
    nco_in.account_identifier;

    nco_in.transaction_id;
    nco_in.location_code;
    nco_in.bk_rule;
    nco_in.sk_rule;
    nco_in.dc_rule;

    // those will always be blank
    batch_in.subj_first_1;
    batch_in.subj_middle_1;
    batch_in.subj_last_1;
    batch_in.subj_suffix_1;
    batch_in.subj_SSN;
    batch_in.subj_DOB;

    batch_address_phone;
    scores;
  end;

  export verification := RECORD (batch_raw)
    layouts.error_report;
  end;

