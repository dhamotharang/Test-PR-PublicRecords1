EXPORT Mask_Billing_Address (DATASET(Layout_HealthFacility) InFile) := FUNCTION
	
	RETURN PROJECT (Infile,TRANSFORM(RECORDOF(Infile), 
		SELF.PRIM_RANGE										:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.PRIM_RANGE); 
		SELF.PREDIR												:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.PREDIR);
		SELF.PRIM_NAME										:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.PRIM_NAME);
		SELF.ADDR_SUFFIX									:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.ADDR_SUFFIX);
		SELF.POSTDIR											:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.POSTDIR);
		SELF.UNIT_DESIG										:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.UNIT_DESIG);
		SELF.SEC_RANGE										:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.SEC_RANGE);
		SELF.P_CITY_NAME									:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.P_CITY_NAME);
		SELF.V_CITY_NAME									:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.V_CITY_NAME);
		SELF.ST														:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.ST);
		SELF.ZIP													:=	IF (LEFT.ADDRESS_CLASSIFICATION = 'M','',LEFT.ZIP);
		SELF := LEFT;));
END;