		shared PatternInvalidWords4 	:= '^SEE FT11 FOR SERVICE OF PROCESS$';
		export InvalidWords						:= PatternInvalidWords1+PatternInvalidWords2+PatternInvalidWords3+PatternInvalidWords4;
	end;

